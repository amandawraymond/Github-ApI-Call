<div class="container">
  <h1 class="text-center"><%=@repos.count%> Ruby repositories found on github </h1>
  <h3 class="text-center">--With more than 5 Forks--</h3>
  <table id="list" class="table table-striped">
    <thead>
        <tr>
            <th>Name</th>
            <th>Description</th>
            <th>Website</th>
        </tr>
    </thead>
    <tbody>
          <%@repos.each do |repo|%>
        <tr>
            <td><%= repo["name"] %></td>
            <td><%= repo["description"]%></td>
            <td><%= repo["html_url"] %></td>
        </tr>
          <%end%>
    </tbody>
  </table>
</div>